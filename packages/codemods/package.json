{
  "name": "@redwoodjs/codemods",
  "version": "8.0.0",
  "description": "Codemods to ease upgrading a RedwoodJS Project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redwoodjs/redwood.git",
    "directory": "packages/codemods"
  },
  "license": "MIT",
  "type": "commonjs",
  "bin": "./dist/codemods.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx ./build.mts",
    "build:pack": "yarn pack -o redwoodjs-codemods.tgz",
    "build:watch": "nodemon --watch src --ignore dist --exec \"yarn build\"",
    "check:package": "yarn publint",
    "generate:codemod": "yarn node ./tasks/generateCodemod/generateCodemod.mjs",
    "prepublishOnly": "yarn build",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@babel/cli": "7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/parser": "7.26.10",
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@babel/runtime-corejs3": "7.26.10",
    "@babel/traverse": "7.26.10",
    "@redwoodjs/project-config": "workspace:*",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@vscode/ripgrep": "1.15.9",
    "@whatwg-node/fetch": "0.9.21",
    "cheerio": "1.0.0",
    "core-js": "3.38.1",
    "deepmerge": "4.3.1",
    "execa": "5.1.1",
    "fast-glob": "3.3.2",
    "graphql": "16.9.0",
    "jscodeshift": "17.0.0",
    "pascalcase": "1.0.0",
    "prettier": "3.4.2",
    "tasuku": "2.0.1",
    "typescript": "5.6.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@redwoodjs/framework-tools": "workspace:*",
    "@types/babel__core": "7.20.5",
    "@types/fs-extra": "11.0.4",
    "@types/jscodeshift": "0.12.0",
    "@types/yargs": "17.0.33",
    "fs-extra": "11.2.0",
    "publint": "0.2.12",
    "tempy": "1.0.1",
    "tsx": "4.19.2",
    "vitest": "2.1.9"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
